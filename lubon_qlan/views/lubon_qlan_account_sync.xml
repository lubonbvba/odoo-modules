<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
		<record model="ir.ui.view" id="tenants_o365_tree_view">
			<field name="name">tenants_o365.tree</field>
			<field name="model">lubon_qlan.tenants_o365</field>
			<field name="arch" type="xml">
				<tree string="Tenants O365 Tree" default_order="name">
					<field name="name"/>
					<field name="defaultdomainname"/>
					<field name="tenant_id"/>
					<field name="qlan_tenant_id"/>
                    <field name="get_details"/>
				</tree>
			</field>
		</record>
		<record model="ir.ui.view" id="tenants_o365_form_view">
			<field name="name">tenants_o365.form</field>
			<field name="model">lubon_qlan.tenants_o365</field>
			<field name="arch" type="xml">
				<form string="Tenants O365 Form" default_order="name">
					<sheet>
						<group>
					<field name="name"/>
					<field name="defaultdomainname"/>
					<field name="tenant_id"/>
					<field name="qlan_tenant_id"/>
					<field name="account_source_id"/>
                    <field name="get_details"/>
					</group>
					<notebook>
						<page string='Subscribed SKUS'>
                            <button name='refresh_subscribed_skus_o365' string="Get subscribed skus" type="object"/>
                            <field name="sku_ids">
										<tree string="SKUS" editable="bottom">
                                            <field name="o365_id"/>
                                            <field name="friendly_name"/>
											<field name="capabilityStatus"/>
											<field name="consumedUnits"/>
											<field name="skuPartNumber"/>
											<field name="skuId"/>
											<field name="enabled"/>
											<field name="suspended"/>
                							<field name="warning"/>
                                        </tree>
                            </field>
						</page>
                        <page string='Users'>
                            <button name='refresh_users_o365' string="Get users" type="object"/>
                            <field name="users_o365_ids">
										<!--tree string="users" editable="bottom">
                                            <field name="o365_id"/>
											<field name="mail"/>
                                            <field name="principalname"/>
                                            <field name="name"/>
											<field name="lastname"/>
											<field name="firstname"/>
											<field name="domainname"/>
             								<field name="qlan_tenant_id"/>
                                        </tree-->
                            </field>
						</page>
					</notebook>
					</sheet>
				</form>
			</field>
		</record>
        <record model="ir.ui.view" id="users_o365_tree_view">
			<field name="name">users_o365.tree</field>
			<field name="model">lubon_qlan.users_o365</field>
			<field name="arch" type="xml">
				<tree string="O365 users Tree" zeditable="top">
                                            <field name="o365_id"/>
											<field name="mail"/>
                                            <field name="principalname"/>
                                            <field name="name"/>
											<field name="lastname"/>
											<field name="firstname"/>
											<field name="domainname"/>
             								<field name="qlan_tenant_id"/>
				</tree>
			</field>
		</record>
		<record model="ir.ui.view" id="users_o365_form_view">
			<field name="name">users_o365.form</field>
			<field name="model">lubon_qlan.users_o365</field>
			<field name="arch" type="xml">
				<form string="Ad accounts">
					<!--header>
						<button string="Passwd reset"/>
						<button string="Disable" type='object' name='disable_user' attrs="{'invisible':[('ad_enabled','=',False)]}"/>
						<button string="Enable" type='object' name='enable_user' attrs="{'invisible':[('ad_enabled','=',True)]}"/>

						<button string="Unlock" attrs="{'invisible':[('ad_lockedout','=',False)]}"/>
						<button name='update_values' type='object' string="Update"/>
						<button name='refresh' type='object' string="Refresh"/>
					</header-->
					<sheet>
						<group>
							<group>
								<field name="person_id" context="{'default_email': principalname, 'default_name': name}"/>
								<field name="name" readonly="1"/>
                            	<field name="principalname" readonly="1"/>
								<field name="o365_tenant_id" readonly="1"/>
                                <field name="qlan_tenant_id" readonly="1"/>
							</group>
						</group>
                  
	          <notebook>
	          	<page string="Licenses">
                  <button name='refresh_licenses' string="Get licenses" type="object"/>
                  <field name="user_licenses_ids">
                    <tree>
                        <field name="user_o365_id"/>
                        <field name="subscribedskus_o365"/>
                    </tree>
                  </field>
	          	</page>
	          	<page string="Billing">
	          	</page>
	          </notebook>
	      </sheet>
	        	<!--div class="oe_chatter">
	        		<field name="message_follower_ids" widget="mail_followers"/>
	        		<field name="message_ids" widget="mail_thread"/>
	        	</div-->
	    		</form>
			</field>
		</record>














	<record model="ir.ui.view" id="lubon_qlan_account_source_type_tree">
        <field name="name">lubon_qlan.account_source_type.tree</field>
        <field name="model">lubon_qlan.account_source_type</field>
        <field name="arch" type="xml">
            <tree string="Source types">
                <field name="name"/>
                <field name="single_tenant"/>
            </tree>
        </field>
    </record>
	<record model="ir.ui.view" id="lubon_qlan_account_source_tree">
        <field name="name">lubon_qlan.account_source.tree</field>
        <field name="model">lubon_qlan.account_source</field>
        <field name="arch" type="xml">
            <tree string="Sources">
                <field name="name"/>
                <field name="tenant_id" invisible="True"/>
                <field name="account_source_type_id"/>
                <field name="include_in_schedule"/>
            </tree>
        </field>
    </record>
    <record model="ir.ui.view" id="lubon_qlan_account_source_form">
        <field name="name">lubon_qlan.account_source.form</field>
        <field name="model">lubon_qlan.account_source</field>
        <field name="arch" type="xml">
            <form string="Sources">
                <header>
                    <button name="run_sync" type="object" string="Run"/>
                </header>
                <group>
                    <group>
                        <field name="name"/>
                        <field name="include_in_schedule"/>
                        <field name="tenant_id" />
                        <field name="account_source_type_id"/>
                        <field name="endpoints_id"/>
                     </group>
                     <group string="Commands"/>
                        <group>
                            <field name="credential_id" context="{'default_model': 'lubon_qlan.account_source','default_related_id': active_id}"/>
                            <field name="tenant_command"/>
                            <field name="user_command"/>
                            <field name="group_command"/>
                        <group/>
                            <group>
                             <button name='get_tenants' string="Get tenants" type="object"/>
                            </group>
                        </group>
                    </group>
                    <group string="Status">
                        <field name="last_full_run_start"/>
                        <field name="last_full_run_stop"/>
                    </group>
                <notebook>
                    <page string="Accounts">
                        <field name="adaccount_ids"/>
                    </page>
                    <page string="Users">
                        <field name="adusers_ids"/>
                    </page>
                    <page string="Groups">
                        <field name="adgroups_ids"/>
                    </page>

                </notebook>    
            </form>
        </field>
    </record>
    <record model="ir.actions.act_window" id="qlan_account_source_type_action">
      <field name="name">Account source types</field>
      <field name="res_model">lubon_qlan.account_source_type</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
    </record>
    <record model="ir.actions.act_window" id="qlan_account_source_action">
      <field name="name">Account source</field>
      <field name="res_model">lubon_qlan.account_source</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
    </record>
    <record model="ir.actions.act_window" id="qlan_users_o365_action">
      <field name="name">O365 users</field>
      <field name="res_model">lubon_qlan.users_o365</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
    </record>
        <menuitem id="lubon_qlan_users_o365_menu" name="O365 Users" parent="lubon_qlan_tables_menu"
          action="qlan_users_o365_action"/>
        <menuitem id="lubon_qlan_account_source_menu" name="Account sources" parent="lubon_qlan_tables_menu"
          action="qlan_account_source_action"/>
        <menuitem id="lubon_qlan_account_source_type_menu" name="Source Types" parent="lubon_qlan_tables_menu"
          action="qlan_account_source_type_action"/>

    </data>
</openerp>